name: blog_entry
backend: postgres(
                schema = erl_db,
                table = my_blog_entry
                )


fields:
    id      :: primary_key(auto_increment, desc="This is so freaking awsome")
    title   :: string(max_length = 255, index, default="Some default title")
    author  :: one_to_many(blog_user.id)
    created :: datetime()
    text    :: string()
